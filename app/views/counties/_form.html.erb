<%= simple_form_for(@county) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :state %>
    <%= f.input :search_url %>
    <%= f.input :search_params %>
    <%= f.input :search_method %>
    <%= f.input :average_days_to_complete %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
