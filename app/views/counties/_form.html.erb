<%= simple_form_for([@state, @county]) do |f| %>
  <div class="row">
    <%= f.error_notification %>
  </div>

	<div class="row"><div class="column_6_centered">
	  <div class="form-inputs">
	    <%= f.input :name %>
	    <%= f.input :search_url %>
	  </div>

	  <div class="form-actions">
	    <div class="column_6">
	      <%= f.button :submit %>
	    </div>
	  </div>
	</div></div>
<% end %>
