<%= simple_form_for([@state, @county]) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :search_url %>
    <%= f.input :search_params %>
    <%= f.input :search_method %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
