<h1><%= @report.job_status.titleize %> Jobs For <%= @report.client.name %></h1>

<table id="jobs">
  <tr>
    <th>File</th>
    <th>Original DOT</th>
    <th>Lender</th>
  </tr>
  <% @report.job_products.each do |job_product| %>
    <tr>
      <td><%= job_product.job.file_number %></td>
      <td><%= job_product.deed_of_trust_number %></td>
      <td><%= job_product.lender.name %></td>
    </tr>
  <% end %>
</table>

