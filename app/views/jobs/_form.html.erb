<%= simple_form_for(@job) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name, label: "Job Name" %>
    <%= f.association :client %>
    <%= f.association :requestor %>
    <%= f.input :address %>
    <%= f.input :city %>
    <%= f.association :state %>
    <%= f.input :zipcode %>
    <%= f.association :county %>
    <%= f.input :old_owner %>
    <%= f.input :new_owner %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
